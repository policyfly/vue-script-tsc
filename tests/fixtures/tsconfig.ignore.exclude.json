{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "src-ignore",
    "strict": true,
    "noEmit": true
  },
  "include": [
    "src-ignore/**/*.ts",
    "src-ignore/**/*.vue"
  ],
  "exclude": [
    "src-ignore/**/Error.vue",
  ]
}
